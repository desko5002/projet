root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{yml,yaml}]
indent_size = 2
$mail->SMTPDebug = 2; // Activer le débogage SMTP
$mail->Debugoutput = 'html'; // Sortie de débogage en HTML

